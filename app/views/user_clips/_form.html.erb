 <%= form_with(model: user_clip, local: true) do |form| %> 
 <% if clip = user_clip.clip %>
  <% if clip.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(clip.errors.count, "error") %>prohibited this user_clip from being saved:</h2>
      <ul>
        <% clip.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
<% end %>
  <div class="field">
    <%= label_tag :url %>
    <%= text_field_tag :url %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
